[access]
	inheritFrom = Dev-ACL-Template
[project]
	description = library to parse AbiWord file-format
[submit]
	action = cherry pick
[access "refs/*"]
	owner = group libabw-admins
	create = group libabw-admins
	push = +force group libabw-admins
	pushMerge = group libabw-admins
	pushTag = group libabw-admins
	pushSignedTag = group libabw-admins
	read = group ReplicateFDO
	read = group ReplicateGithub
